<resources>

    <!--价格选择SeekBar-->
    <declare-styleable name="RangeSeekBar">
        <attr name="max" format="float"/>
        <attr name="min" format="float"/>
        <attr name="reserve" format="float"/>
        <attr name="cells" format="integer"/>
        <attr name="lineColorSelected" format="color"/>
        <attr name="lineColorEdge" format="color"/>
        <attr name="seekBarResId" format="reference"/>
    </declare-styleable>

    <!--价格显示控件-->
    <declare-styleable name="HotelPriceView">
        <attr name="fontSize" format="dimension"/>
        <attr name="selectColor" format="color"/>
        <attr name="unSelectColor" format="color"/>
        <attr name="price" format="string"/>
        <attr name="pointSize" format="dimension"/>
    </declare-styleable>

</resources>
